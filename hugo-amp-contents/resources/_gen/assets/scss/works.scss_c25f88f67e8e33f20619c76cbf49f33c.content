:root{--color-error: 204, 85, 85}@media (prefers-color-scheme: light){:root{--color-theme: 255,204,102;--color-base: 255,255,255;--color-main: 34,51,102}}@media (prefers-color-scheme: dark){:root{--color-theme: 238,187,102;--color-base: 30, 40, 77;--color-main: 255,255,255}}html{background:rgba(var(--color-base), 1);font-size:11px}@media screen and (max-width: 1440px){html{font-size:10px}}@media screen and (max-width: 960px){html{font-size:9.5px}}@media screen and (max-width: 480px){html{font-size:9px}}body{margin:0;padding:0;font-size:1.6rem;color:rgba(var(--color-main), 1);line-height:1.8;font-weight:normal;font-family:"Avenir Next", "Avenir", "SF Pro Text", "Robot", "Helvetica Neue", "Helvetica", "Arial", "Hiragino Sans", -apple-system, BlinkMacSystemFont, "Hiragino Kaku Gothic ProN", "Noto Sans JP", "Noto Sans CJK JP", "Meiryo", "メイリオ", "Segoe UI", sans-serif;font-smoothing:antialiased;font-feature-settings:"pkna";letter-spacing:0.05em;-webkit-text-size-adjust:100%}*,*::before,*::after{margin:0;padding:0;box-sizing:border-box}ul{list-style-type:none}img{border:none;vertical-align:top;max-width:100%}b,strong{font-weight:bolder}a{color:inherit;text-decoration:none;transition:0.2s ease-out}p a{text-decoration:underline}button{cursor:pointer;background:none;border:none;outline:none;font-size:inherit;color:inherit;transition:0.2s ease-out}a,area,button,[role="button"],input:not([type="range"]),label,select,summary,textarea{touch-action:manipulation}.material-icons-round{font-size:1.2em;vertical-align:-.2em}
