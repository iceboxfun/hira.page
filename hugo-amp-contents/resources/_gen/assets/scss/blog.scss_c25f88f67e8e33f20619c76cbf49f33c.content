ansform:scale(1.01)}.embed-card *{position:absolute}.embed-card .og{width:16rem;height:12rem;margin:0;top:0;left:0}.embed-card .og img{object-fit:cover}@media screen and (max-width: 480px){.embed-card .og{width:10rem}}.embed-card h5{display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;overflow:hidden;top:1.8rem;left:18rem;font-size:1.6rem;width:calc( 100% - 20rem);line-height:1.4;color:rgba(var(--color-main), 0.8);font-weight:600}@media screen and (max-width: 480px){.embed-card h5{top:1.6rem;left:11.6rem;width:calc( 100% - 13.2rem)}}.embed-card .favi{left:18rem;bottom:1.6rem;width:2rem;height:2rem}@media screen and (max-width: 480px){.embed-card .favi{left:11.6rem;bottom:1.2rem}}.embed-card span{left:21rem;bottom:1.6rem;width:calc( 100% - 23rem);overflow:hidden;white-space:nowrap;text-overflow:ellipsis;line-height:2rem;color:rgba(var(--color-main), 0.6)}@media screen and (max-width: 480px){.embed-card span{left:14.4rem;bottom:1.2rem}}.share{display:flex;position:fixed;flex-direction:column;bottom:2rem;right:2rem}@media (max-width: 960px){.share{flex-direction:row}}.share a{text-indent:100%;white-space:nowrap;overflow:hidden;margin-top:1.4rem;margin-right:1.4rem;display:block;width:4rem;height:4rem;border-radius:50%;background:url("/img/blog-share.svg") no-repeat;background-color:rgba(var(--color-main), 0.3);backdrop-filter:blur(8px);background-size:28rem 4rem}.share a.twitter{background-position:0 0}.share a.twitter:hover{background-color:#008ff7}.share a.facebook{background-position:-4rem 0}.share a.facebook:hover{background-color:#1977f2}.share a.hatena{background-position:-8rem 0}.share a.hatena:hover{background-color:#00a4de}.share a.pocket{background-position:-12rem 0}@media (max-width: 480px){.share a.pocket{display:none}}.share a.pocket:hover{background-color:#ef4056}.share a.line{background-position:-16rem 0;display:none}@media (max-width: 480px){.share a.line{display:block}}.share a.line:hover{background-color:#00B500}.follow h3{margin-top:1